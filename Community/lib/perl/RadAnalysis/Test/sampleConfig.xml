<plugin>
  <process class="GUS::Community::RadAnalysis::Processer::GeneConfTwoClassPaGE">

    <!-- the basics -->
    <property name="pageInputFile" value="pru_vs_rh_page.txt"/>
    <property name="studyName" value="Expression profiling of the 3 archetypal T. gondii lineages"/>
    <property name="arrayDesignName" value="Tgondiia Custom Array - Expression"/>
    <property name="nameConditionA" value="Pru - RMA Quantifications"/>
    <property name="nameConditionB" value="RH - RMA Quantifications"/>

    <!-- PaGE Params which the user must specify -->
    <property name="baseX" value="2"/>
    <property name="numberOfChannels" value="1"/>
    <property name="isDataLogged" value="1"/>
    <property name="isDataPaired" value="0"/>

    <!-- Quantifications (NOTE:  If data is paired, the order below matters!! -->
    <property name="quantificationView" value="RMAExpress"/>
    <property name="quantificationUrisConditionA">
      <value>Pru_1.TXT</value>
      <value>Pru_2.TXT</value>
      <value>Pru_3.TXT</value>
    </property>
    <property name="quantificationUrisConditionB">
      <value>RH_1.TXT</value>
      <value>RH_2.TXT</value>
      <value>RH_3.TXT</value>
    </property>

  </process>
  <process class="GUS::Community::RadAnalysis::Processer::GeneConfTwoClassPaGE">

    <property name="pageInputFile" value="pru_vs_ctg_page.txt"/>
    <property name="baseX" value="2"/>
    <property name="studyName" value="Expression profiling of the 3 archetypal T. gondii lineages"/>
    <property name="arrayDesignName" value="Tgondiia Custom Array - Expression"/>
    <property name="nameConditionA" value="Pru - RMA Quantifications"/>
    <property name="nameConditionB" value="CTG - RMA Quantifications"/>
    <property name="numberOfChannels" value="1"/>
    <property name="isDataLogged" value="1"/>
    <property name="isDataPaired" value="0"/>
    <property name="quantificationView" value="RMAExpress"/>
    <property name="quantificationUrisConditionA">
      <value>Pru_1.TXT</value>
      <value>Pru_2.TXT</value>
      <value>Pru_3.TXT</value>
    </property>
    <property name="quantificationUrisConditionB">
      <value>CTG_1.TXT</value>
      <value>CTG_2.TXT</value>
    </property>
  </process>

  <process class="GUS::Community::RadAnalysis::Processer::GeneConfTwoClassPaGE">
    <property name="pageInputFile" value="rh_vs_ctg_page.txt"/>
    <property name="baseX" value="2"/>
    <property name="studyName" value="Expression profiling of the 3 archetypal T. gondii lineages"/>
    <property name="arrayDesignName" value="Tgondiia Custom Array - Expression"/>
    <property name="nameConditionA" value="RH - RMA Quantifications"/>
    <property name="nameConditionB" value="CTG - RMA Quantifications"/>
    <property name="numberOfChannels" value="1"/>
    <property name="isDataLogged" value="1"/>
    <property name="isDataPaired" value="0"/>
    <property name="quantificationView" value="RMAExpress"/>
    <property name="quantificationUrisConditionA">
      <value>RH_1.TXT</value>
      <value>RH_2.TXT</value>
      <value>RH_3.TXT</value>
    </property>
    <property name="quantificationUrisConditionB">
      <value>CTG_1.TXT</value>
      <value>CTG_2.TXT</value>
    </property>
  </process>

</plugin>
